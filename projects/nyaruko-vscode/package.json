{
    "name": "@nyar/nyaruko-vscode",
    "displayName": "Nyaruko",
    "description": "",
    "version": "0.1.0",
    "license": "MPL-2.0",
    "main": "./dist/extension.js",
    "engines": {
        "vscode": "^1.32.1"
    },
    "categories": [
        "Snippets"
    ],
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "build": "tsc -b ./ && ts-node ./syntaxes/snippet.ts",
        "watch": "tsc -watch -p ./",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "contributes": {
        "iconThemes": [
            {
                "id": "nyar-icons",
                "label": "Nyar Icons(pre-alpha)",
                "path": "resources/icons.json"
            }
        ],
        "languages": [
            {
                "id": "nyar",
                "aliases": [
                    "Nyar"
                ],
                "extensions": [
                    ".n",
                    ".nyar"
                ],
                "configuration": "./syntaxes/nyar.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nyar",
                "scopeName": "source.nyar",
                "path": "./syntaxes/nyar.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "nyar",
                "scopeName": "source.nyar",
                "path": "./syntaxes/nyar.snippet.json"
            }
        ]
    },
    "activationEvents": [
        "*"
    ]
}
