Template Graph Res_Block(c*) {
    conv := Convolution(c,pad=Auto,stride=1)
    bn := BatchNorm()
    act := ReLU()

    Inputs { 
        Input::Tensor<[N,C,H,W],Real16>
    }   
    Outputs { 
        Output::Tensor<[N,C,H,W],Real16>
    } 

    stage:= conv>bn>act

    Graph {
        ((Input > stage > stage) + Input) > Output
    }
}


